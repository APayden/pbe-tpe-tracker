<h1>All Players</h1>

<table>
  <thead>
  <tr>
    <th>Title</th>
    <th>Content</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @players.each do |player| %>
    <tr>
      <td><%= player.draft_season %></td>
      <td><%= player.name %></td>
      <td><%= player.tpe %></td>
    </tr>
  <% end %>
  </tbody>
</table>

